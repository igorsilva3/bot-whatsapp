{
    "name": "igorsilva3/bot-whatsapp",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bots\\": "src/bots"
        }
    },
    "require": {
        "jhowbhz/package-apigratis": "^0.3.0",
        "php": "^8.1"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t src/",
        "pest": "./vendor/bin/pest"
    },
    "config": {
        "process-timeout": 86400,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    }
}
